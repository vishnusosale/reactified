{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [0, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": [0, { "ignorePureComponents": true }]
  },
  "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
  },
  "plugins": [
      "react"
  ],
}
